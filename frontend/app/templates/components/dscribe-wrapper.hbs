<h2>{{image.num}}</h2>
<div class="form-horizontal">
  <div class="col-sm-12 text-center" style="padding-bottom:10px">
    {{#if small}}
      <a style="color:black;" href="{{fileUrl}}" target="_blank">
        <img src="data:image/jpeg;base64,{{small}}">
      </a>
    {{/if}}
  </div>
  <div class="form-group">
    <label class="control-label col-sm-2">Physical</label>
      <div class="col-sm-10">
  {{#x-select value=image.page_type class="form-control"}}
    {{#x-option value='undefined'}}Undefined{{/x-option}}
    {{#x-option value='BookCover'}}BookCover{{/x-option}}
    {{#x-option value='LeftPage'}}LeftPage{{/x-option}}
    {{#x-option value='RightPage'}}RightPage{{/x-option}}
    {{#x-option value='DoublePage'}}DoublePage{{/x-option}}
    {{#x-option value='LoosePage'}}LoosePage{{/x-option}}
    {{#x-option value='FoldOut'}}FoldOut{{/x-option}}
    {{#x-option value='ColorTarget'}}ColorTarget{{/x-option}}
    {{#x-option value='BookSpine'}}BookSpine{{/x-option}}
    {{/x-select}}
      </div>
  </div>
  <div class="form-group">
    <label class="control-label col-sm-2">Logical</label>
      <div class="col-sm-10">
  {{#x-select value=image.page_content class="form-control"}}
    {{#x-option value='undefined'}}Undefined{{/x-option}}
    {{#x-option value='TitlePage'}}TitlePage{{/x-option}}
    {{#x-option value='OriginalCover'}}OriginalCover{{/x-option}}
    {{#x-option value='TableOfContents'}}TableOfContents{{/x-option}}
    {{#x-option value='Index'}}Index{{/x-option}}
    {{#x-option value='EmptyPage'}}EmptyPage{{/x-option}}
    {{#x-option value='Illustration'}}Illustration{{/x-option}}
    {{#x-option value='PhotographicIllustration'}}PhotographicIllustration{{/x-option}}
  {{/x-select}}
      </div>
  </div>
</div>
<hr/>
